<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.VBox?>
<BorderPane id="box" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.kryptojagd.controls.LevelFinished">
   <center>
      <VBox alignment="CENTER" prefHeight="200.0" prefWidth="100.0" spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="text" text="Text" BorderPane.alignment="CENTER"/>
            <Label fx:id="score" text="Label" BorderPane.alignment="CENTER" />
         </children>
      </VBox>

   </center>
   <bottom>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <Button id="Menu" fx:id="Menu" mnemonicParsing="false" onAction="#menuAction" text="Menü">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Button>
            <Button id="NextLevel" fx:id="NextLevel" contentDisplay="CENTER" mnemonicParsing="false" onAction="#nextLevelAction" text="Nächstes Level">
               <HBox.margin>
                  <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </bottom>
   <!--DO NOT TOUCH-->
   <top>
      <BorderPane prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <right>
            <Label fx:id="timer" text="" BorderPane.alignment="CENTER" />
         </right>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </BorderPane>
   </top>
   <!--DO NOT TOUCH-->
</BorderPane>
