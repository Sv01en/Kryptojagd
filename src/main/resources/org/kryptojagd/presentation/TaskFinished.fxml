<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane id="box" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.kryptojagd.controls.TaskFinishedController">
   <center>
      <Label fx:id="feedbackText" text="Falsche Auswahl" BorderPane.alignment="CENTER" />
   </center>
   <bottom>
      <Button mnemonicParsing="false" onAction="#nextWindow" text="Weiter" BorderPane.alignment="BOTTOM_CENTER" />
   </bottom>
   <!--DO NOT TOUCH-->
   <top>
      <BorderPane prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <right>
            <Label fx:id="timer" text="" BorderPane.alignment="CENTER" />
         </right>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
      </BorderPane>
   </top>
   <!--DO NOT TOUCH-->
   <opaqueInsets>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </opaqueInsets>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
   <left>
      <Label fx:id="score" prefHeight="110.0" prefWidth="200.0" text="Label" BorderPane.alignment="CENTER" />
   </left>
</BorderPane>
