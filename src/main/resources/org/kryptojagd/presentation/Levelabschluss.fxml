<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane id="box" prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.kryptojagd.controls.LevelFinishedController">
   <center>
      <Label text="Herzlichen Glückwunsch! Du hast das Level gelöst." BorderPane.alignment="CENTER" />
   </center>
   <bottom>
      <Button fx:id="nextButton" mnemonicParsing="false" onAction="#clickNext" text="Weiter" BorderPane.alignment="BOTTOM_CENTER" />
   </bottom>
   <opaqueInsets>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </opaqueInsets>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</BorderPane>
