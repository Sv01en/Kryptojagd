<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane id="box" prefHeight="800.0" prefWidth="1300.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.kryptojagd.controls.MistakeMessageController">
<center>
   <Label fx:id="message1" BorderPane.alignment="CENTER" />
</center>
<bottom>
   <Button fx:id="nextButton" mnemonicParsing="false" onAction="#clickNext" text="Weiter" BorderPane.alignment="BOTTOM_CENTER" />
</bottom>
   <!--DO NOT TOUCH-->
   <top>
      <BorderPane prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <right>
            <Label fx:id="timer" text="" BorderPane.alignment="CENTER" />
         </right>
         <padding>
            <Insets left="10.0" right="10.0" />
         </padding>
         <center>
            <Label fx:id="score" text="Label" BorderPane.alignment="CENTER" />
         </center>
      </BorderPane>
   </top>
   <!--DO NOT TOUCH-->
<opaqueInsets>
   <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
</opaqueInsets>
<padding>
   <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
</padding>
</BorderPane>
